    import mne, numpy, pandas, xarray
    print('‚úì Python –ø–∞–∫–µ—Ç—ã —Ä–∞–±–æ—Ç–∞—é—Ç –∫–æ—Ä—Ä–µ–∫—Ç–Ω–æ')
